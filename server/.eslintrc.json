{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true,
		"commonjs": true
	},
	"extends": [
		"eslint:recommended",
		"eslint:recommended",
		"plugin:prettier/recommended"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"endOfLine": "auto"
			}
		],
		"default-param-last": "off"
	}
}
